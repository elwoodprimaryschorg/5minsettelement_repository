<mvc:View id="idStuHeaderView" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="studentRegisterAppDescNs.studentRegisterAdPn.controller.stuHeader" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="idStuHeaderPage" title="Students">
		<content>
			<List id="idList" items="{/students}" mode="SingleSelectMaster" selectionChange="onItemPress">
				<ObjectListItem intro="{stuName}" number="{stuId}"/>
			</List>
		</content>
	</Page>
</mvc:View>